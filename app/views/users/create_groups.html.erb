<h2> 作成チーム一覧</h2>
<div class="row">
  <div class="col-10 offset-1">
    <table class="table">
      <thead>
        <tr>
          <th>チーム名</th>
          <th>作成日</th>
          <th></th>
        </tr>
      </thead>
      <% @create_groups.each do |create_group|%>
        <tbody>
          <tr>
            <td>
              <%= create_group.group.name %>
            </td>
            <td><%= create_group.group.created_at.strftime("%Y年%m月%d日") %></td>
            <td>
              <%= link_to "詳細へ", group_path(create_group.group.id) %>
            </td>
          </tr>
        </tbody>
      <% end %>
    </table>
    <%= paginate @create_groups, class: "paginate" %>
  </div>
</div>